#!/bin/bash
if [ $EUID != 0 ]
then
	printf "Needs to be run as root/sudo user.\n"
	exit 1;
fi

#COLORS
RED='\033[0;31m'
NC='\033[0m' #No color
GREEN='\033[1;32m'
YLLW='\033[1;33m'
DG='\033[0;37m'
LR='\033[0;31m'
patch=5.01
#END COLORS

nmapipscan(){
	whereah=`which autohack`; cd ${whereah::-8}; cd scripts/scanning_enumeration
	./nmapipscan
	cd ../..
}
