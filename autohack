macspoof(){
printf "Are you using eth0 or wlan0? ---> "
read interface
gnome-terminal --command="bash -c 'sudo su; ifconfig $interface down; macchanger -r $interface; ifconfig $interface up; macchanger -s $interface; $SHELL'"
#ifconfig $interface down
#macchanger -r $interface 
#ifconfig $interface up
#macchanger -s $interface

}
smbsigning(){
printf "Enter Target IP Address or Subnet ---> "
read victim
gnome-terminal --command="bash -c 'nmap --script=smb2-security-mode.nse -p139,445 $victim; $SHELL'"
}
autorecon(){
printf "Enter Target IP Address ---> "
read targip
printf "Name the Directory for the ouput ---> "
read folder
printf ${GREEN}"This tool is powered by the amazing github repo: AutoRecon by Tib3rius. If you don't have it and the dependencies of that project installed. You can find and download the project here ---> https://github.com/Tib3rius/AutoRecon"${NC}
gnome-terminal --command="bash -c 'autorecon $targip -o $folder; $SHELL'"
}
wpscan(){
printf "Enter Target URL ---> "
read targurl
printf "Would you like to save the output to a new text file?(y/n)"
read choice
if [ $choice == y ]
then
	printf "Name for output file: "
	read outputfile
	gnome-terminal --command="bash -c 'wpscan --url $targurl -o $outputfile; $SHELL'"
else
	gnome-terminal --command="bash -c 'wpscan --url $targurl --enumerate u,ap,tt,t; $SHELL'"
fi
}
dependencies_installer(){
#THIS IS DEF OVERKILL
apt-get install gobuster

apt-get install gnome-terminal

apt install seclists curl enum4linux feroxbuster nbtscan nikto nmap onesixtyone oscanner smbclient smbmap smtp-user-enum snmp sslscan sipvicious tnscmd10g whatweb wkhtmltopdf
}
scouter(){
printf "Enter target BSSID ---> "
read bssid
printf "Enter Channel ----> "
read channel
gnome-terminal --command="bash -c 'airodump-ng --bssid $bssid --channel $channel wlan0mon; $SHELL'"
}
